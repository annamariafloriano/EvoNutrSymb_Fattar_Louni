mv OG0000111.aln.fst.ren_2.fst ../Francisella_concat.fst
cd ..

modeltest-ng -i Francisella_concat.fst -d nt -o Francisella_concat.fst.model -p 6 ## GTR+I+G4
raxmlHPC-PTHREADS -T 20 -f a -s Francisella_concat.fst -n Francisella_concat.fst.boot -x 1234 -# 100 -p 123 -m GTRGAMMAIX

############################################## CLEs
for i in ready/*.gbk; do python3 pseudofinder-master/pseudofinder.py annotate -g $i -db Coxiella_ref_db.faa -op $i.pf; done

## get intact genes in intactgenes folder
cp ready/*intact*ffn intactgenes/


###### phylo:
cd intactgenes/
for i in *.ffn; do python3 ../renameFASTAheaders_byFilename.py -f $i > $i.ren.fa; done
mkdir orifiles
mv *.ffn orifiles/
for i in *ren.fa; do seaview -convert -translate $i; done
mkdir nt_ren
mv *.fa nt_ren/
## change fst to faa
cd ..
python3 OrthoFinder_source/orthofinder.py -f intactgenes/

cd intactgenes/OrthoFinder/
mkdir Results_Sep10_analysis
cd Results_Sep10_analysis
cp ../Results_Sep10/Orthogroups/Orthogroups.GeneCount.tsv .

bash printMultLines.1.sh > 0.tmp
paste -d " " org.lst 0.tmp > rcommsel.txt ## modify to get command (see SelComm.R)
R ###
cut -f 1 SelClusters.tsv | sed '1d' > SelClusters.lst
mkdir SelClusters

bash printMultLines.2.sh > 1.tmp
bash printMultLines.3.sh > 3.tmp
paste 1.tmp SelClusters.lst > 2.tmp ## replace \t with nothing, replace \n with .fa\n, last one has to be added manually
paste -d " " 2.tmp 3.tmp > copy.sh
bash copy.sh
mkdir nt_data
cp ../../nt_ren/* nt_data/
python3 ../../../GetNtClusters.py SelClusters nt_data SelClusters_nt

for i in SelClusters_nt/*.fa; do seaview -align -align_at_protein_level $i; done ## rename .fst to .aln.fst to keep track
mkdir data_nt_ren
cp SelClusters_nt/*aln.fst data_nt_ren/

for i in data_nt_ren/*aln.fst; do python3 ../../../changeHeaders.py $i $i.ren.fst; done
rm data_nt_ren/*aln.fst
