############################################## FLEs

## find intact genes with PseudoFinder 
for i in ready/*.gbk; do python3 pseudofinder-master/pseudofinder.py annotate -g $i -db Francisella_ref_db.faa -op $i.pf; done
mkdir intactgenes
cp ready/*intact*ffn intactgenes/

###### phylo:
#### prepare the files
cd intactgenes/
for i in *.ffn; do python3 ../renameFASTAheaders_byFilename.py -f $i > $i.ren.fa; done
mkdir orifiles
mv *.ffn orifiles/
for i in *ren.fa; do seaview -convert -translate $i; done
mkdir nt_ren
mv *.fa nt_ren/
## change fst to faa
cd ..

#### find orthologs for phylo
python3 OrthoFinder_source/orthofinder.py -f intactgenes/

cd intactgenes/OrthoFinder/
mkdir Results_Sep10_analysis
cd Results_Sep10_analysis
cp ../Results_Sep10/Orthogroups/Orthogroups.GeneCount.tsv .

bash printMultLines.1.sh > 0.tmp
paste -d " " org.lst 0.tmp > rcommsel.txt ## modify to get command (see SelComm.R)
R ###
cut -f 1 SelClusters.tsv | sed '1d' > SelClusters.lst
mkdir SelClusters

bash printMultLines.2.sh > 1.tmp
bash printMultLines.3.sh > 3.tmp
paste 1.tmp SelClusters.lst > 2.tmp ## replace \t with nothing, replace \n with .fa\n, last one has to be added manually
paste -d " " 2.tmp 3.tmp > copy.sh
bash copy.sh
mkdir nt_data
cp ../../nt_ren/* nt_data/
python3 ../../../GetNtClusters.py SelClusters nt_data SelClusters_nt

for i in SelClusters_nt/*.fa; do seaview -align -align_at_protein_level $i; done ## rename .fst to .aln.fst to keep track
mkdir data_nt_ren
cp SelClusters_nt/*aln.fst data_nt_ren/

for i in data_nt_ren/*aln.fst; do python3 ../../../changeHeaders.py $i $i.ren.fst; done
rm data_nt_ren/*aln.fst
ls data_nt_ren/* | cut -d "/" -f2 > aln_files.lst
cd data_nt_ren/

seaview -concatenate OG0000112.aln.fst.ren.fst,OG0000113.aln.fst.ren.fst,OG0000114.aln.fst.ren.fst,OG0000115.aln.fst.ren.fst,OG0000116.aln.fst.ren.fst,OG0000117.aln.fst.ren.fst,OG0000118.aln.fst.ren.fst,OG0000119.aln.fst.ren.fst,OG0000120.aln.fst.ren.fst,OG0000121.aln.fst.ren.fst,OG0000122.aln.fst.ren.fst,OG0000123.aln.fst.ren.fst,OG0000124.aln.fst.ren.fst,OG0000125.aln.fst.ren.fst,OG0000126.aln.fst.ren.fst,OG0000127.aln.fst.ren.fst,OG0000128.aln.fst.ren.fst,OG0000129.aln.fst.ren.fst,OG0000130.aln.fst.ren.fst,OG0000131.aln.fst.ren.fst,OG0000132.aln.fst.ren.fst,OG0000133.aln.fst.ren.fst,OG0000134.aln.fst.ren.fst,OG0000135.aln.fst.ren.fst,OG0000136.aln.fst.ren.fst,OG0000137.aln.fst.ren.fst,OG0000138.aln.fst.ren.fst,OG0000139.aln.fst.ren.fst,OG0000140.aln.fst.ren.fst,OG0000141.aln.fst.ren.fst,OG0000142.aln.fst.ren.fst,OG0000143.aln.fst.ren.fst,OG0000144.aln.fst.ren.fst,OG0000145.aln.fst.ren.fst,OG0000146.aln.fst.ren.fst,OG0000147.aln.fst.ren.fst,OG0000148.aln.fst.ren.fst,OG0000149.aln.fst.ren.fst,OG0000150.aln.fst.ren.fst,OG0000151.aln.fst.ren.fst,OG0000152.aln.fst.ren.fst,OG0000153.aln.fst.ren.fst,OG0000154.aln.fst.ren.fst,OG0000155.aln.fst.ren.fst,OG0000156.aln.fst.ren.fst,OG0000157.aln.fst.ren.fst,OG0000158.aln.fst.ren.fst,OG0000159.aln.fst.ren.fst,OG0000160.aln.fst.ren.fst,OG0000161.aln.fst.ren.fst,OG0000162.aln.fst.ren.fst,OG0000163.aln.fst.ren.fst,OG0000164.aln.fst.ren.fst,OG0000165.aln.fst.ren.fst,OG0000166.aln.fst.ren.fst,OG0000167.aln.fst.ren.fst,OG0000168.aln.fst.ren.fst,OG0000169.aln.fst.ren.fst,OG0000170.aln.fst.ren.fst,OG0000171.aln.fst.ren.fst,OG0000172.aln.fst.ren.fst,OG0000173.aln.fst.ren.fst,OG0000174.aln.fst.ren.fst,OG0000175.aln.fst.ren.fst,OG0000176.aln.fst.ren.fst,OG0000177.aln.fst.ren.fst,OG0000178.aln.fst.ren.fst,OG0000179.aln.fst.ren.fst,OG0000180.aln.fst.ren.fst,OG0000181.aln.fst.ren.fst,OG0000182.aln.fst.ren.fst,OG0000183.aln.fst.ren.fst,OG0000184.aln.fst.ren.fst,OG0000185.aln.fst.ren.fst,OG0000186.aln.fst.ren.fst,OG0000187.aln.fst.ren.fst,OG0